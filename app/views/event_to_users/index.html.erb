<p id="notice"><%= notice %></p>

<h1>Event To Users</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_to_users.each do |event_to_user| %>
      <tr>
        <td><%= event_to_user.event.name %></td>
        <td><%= event_to_user.user.first_name %></td>
        <td><%= link_to 'Show', event_to_user %></td>
        <td><%= link_to 'Edit', edit_event_to_user_path(event_to_user) %></td>
        <td><%= link_to 'Destroy', event_to_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event To User', new_event_to_user_path %>
